{
    "name": "laravel/todolist",
    "description": "You can create the to-do-list of your task.",
    "type": "composer-plugin",
    "keywords": [
        "laravel",
        "plugin",
        "todo",
        "todolist"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Vaidotas Poskus",
            "email": "vaidotas.poskus1@gmail.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1.3",
        "laravelcollective/html": "^5.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "mockery/mockery": "^1.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravel\\Todolist\\TodolistServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Todolist\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\Todolist\\Test\\": "tests"
        }
    }
}
